    </header>

    <header
            android:icon="@drawable/ic_launcher"
            android:title="transitions"
            android:summary="Transition Animation测试">

        <intent
                android:targetClass="xzy.demo.androidtestapi.transition.TestTransition"
                android:targetPackage="xzy.demo.androidtestapi"/>

    </header>

