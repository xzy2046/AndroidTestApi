<preference-headers
        xmlns:android="http://schemas.android.com/apk/res/android">

    <header
            android:icon="@drawable/ic_launcher"
            android:title="时间矫正"
            android:summary="手机时间/时区错误的矫正">

        <extra android:name="someKey" android:value="someHeaderValue"/>
        <intent
                android:targetClass="xzy.demo.androidtestapi.time.TimeZoneActivity"
                android:targetPackage="xzy.demo.androidtestapi"/>
    </header>

    <header
            android:icon="@drawable/ic_launcher"
            android:title="RecyclerViewDemo"
